<% content_for :head do %>
	<title>Sign up</title>
<% end %>

<% content_for :body do %>
	<%= form_for(User.new, as: :user, url: travels_registration_path(:user)) do |f| %>
	  

	  <div class="field">
		<%= f.label :phone_number %><br />
		<%= f.text_field :phone_number, autofocus: true %>
	  </div>
	  <div class="actions">
		<%= f.submit "Sign up" %>
	  </div>
	<% end %>

	<%= render "devise/shared/links" %>

<% end %>
