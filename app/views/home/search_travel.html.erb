<%= content_for :body do %>
<h1>Listing Travels</h1>
<thead>
<table>
  <th>Dep address</th>
      <th>Stage address</th>
      <th>Arr address</th>
      <th>Arr lat</th>
      <th>Arr lng</th>
      <th>Dep time</th>
      <th>Arr time</th>
      <th>Stage time</th>
      <th>Distance</th>
      <th>way</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <% if @travels.nil? %>
  	<p> Non ci sono viaggi</p>
  	<% else %>
    <% @travels.uniq.each do |travel| %>
      <tr>
        <td><%= travel.dep_address %></td>
        <td><%= travel.stage_address %></td>
        <td><%= travel.arr_address %></td>
        <td><%= travel.dep_time %></td>
        <td><%= travel.arr_time %></td>
        <td><%= travel.stage_time %></td>
        <td><%= travel.distance %></td>
        <td><%= travel.time %></td>
         <td><%= travel.way %></td>       
        <td><%= link_to 'Show', travel %></td>
        <td><%= link_to 'Edit', edit_travel_path(travel) %></td>
        <td><%= link_to 'Destroy', travel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

<br>
<% end %>
